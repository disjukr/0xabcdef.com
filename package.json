{
    "name": "homepage",
    "private": true,
    "author": "JongChan Choi <jong@chan.moe>",
    "license": "UNLICENSED",
    "scripts": {
        "clear": "rimraf .cache public",
        "gatsby": "cross-env GATSBY_GRAPHQL_IDE=playground sucrase-node ./node_modules/gatsby/dist/bin/gatsby.js",
        "prettier": "prettier --write \"*.@(ts|json)\" \"@(src|types)/**/*.ts?(x)\"",
        "gh-pages": "gh-pages -d public -b master",
        "deploy": "run-s build gh-pages",
        "build": "npm run clear && npm run gatsby -- build",
        "dev": "run-p dev:**",
        "dev:gatsby": "npm run gatsby -- develop",
        "dev:prettier": "onchange \"@(*.ts|@(src|types)/**/*.@(ts|tsx|scss))\" -- prettier --write {{changed}}"
    },
    "dependencies": {
        "@mdx-js/mdx": "^0.16.8",
        "@mdx-js/tag": "^0.16.8",
        "@types/classnames": "^2.2.7",
        "@types/pixi.js": "^4.8.6",
        "@types/react-helmet": "^5.0.8",
        "@types/react-transition-group": "^2.0.15",
        "@types/three": "^0.93.19",
        "classnames": "^2.2.6",
        "cross-env": "^5.2.0",
        "gatsby": "^2.0.118",
        "gatsby-mdx": "^0.3.5",
        "gatsby-plugin-layout": "^1.0.12",
        "gatsby-plugin-manifest": "^2.0.17",
        "gatsby-plugin-react-helmet": "^3.0.6",
        "gatsby-plugin-sass": "^2.0.10",
        "gatsby-plugin-typescript": "^2.0.7",
        "gh-pages": "^2.0.1",
        "join-react-context": "0.0.3",
        "node-sass": "^4.11.0",
        "npm-run-all": "^4.1.5",
        "onchange": "^5.2.0",
        "pixi.js": "^4.8.5",
        "prettier": "^1.16.4",
        "react": "^16.8.1",
        "react-dom": "^16.8.1",
        "react-helmet": "^6.0.0-beta",
        "react-transition-group": "^2.5.3",
        "rimraf": "^2.6.3",
        "sucrase": "^3.9.5",
        "three": "^0.101.1"
    }
}
